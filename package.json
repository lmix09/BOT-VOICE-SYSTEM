{
  "name": "stay-voice-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "pm2:start": "pm2 start src/index.js --name stay-voice-bot",
    "pm2:restart": "pm2 restart stay-voice-bot",
    "pm2:stop": "pm2 stop stay-voice-bot",
    "pm2:logs": "pm2 logs stay-voice-bot --lines 100",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "build:exe": "pkg -t node22-win-x64 src/index.js -o START.exe"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.8",
    "discord.js": "^14.16.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}